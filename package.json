{
  "name": "cornerbycorner",
  "version": "1.0.0",
  "description": "Personal website built with Eleventy",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf _site css",
    "css:clean": "node scripts/clean-css-duplicates.js",
    "start": "npm run clean && npm run css:clean && eleventy --serve",
    "build": "npm run clean && npm run css:clean && eleventy",
    "build:webpack": "webpack --mode production",
    "build:eleventy": "eleventy",
    "debug": "DEBUG=* eleventy",
    "dev": "npm run css:clean && eleventy --serve",
    "dev:webpack": "webpack --mode development --watch",
    "dev:eleventy": "eleventy --serve",
    "css": "node scripts/process-css.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/username/cornerbycorner.org.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@11ty/eleventy": "^2.0.0",
    "@11ty/eleventy-plugin-rss": "^1.2.0",
    "@11ty/eleventy-plugin-syntaxhighlight": "^5.0.0",
    "@babel/core": "^7.22.1",
    "@babel/preset-env": "^7.22.4",
    "autoprefixer": "^10.4.21",
    "babel-loader": "^9.1.2",
    "clean-css": "^5.3.3",
    "copy-webpack-plugin": "^13.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.8.1",
    "css-minimizer-webpack-plugin": "^5.0.1",
    "html-minifier": "^4.0.0",
    "html-webpack-plugin": "^5.6.3",
    "luxon": "^3.3.0",
    "mini-css-extract-plugin": "^2.9.2",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.31",
    "postcss-loader": "^7.3.3",
    "rimraf": "^3.0.2",
    "terser-webpack-plugin": "^5.3.9",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4"
  }
